      function initMap() {
        var myCenter = {lat: 37.934802, lng: -122.308582};
        var mapDiv = document.getElementById('map');
        var map = new google.maps.Map(mapDiv, {
            center: myCenter,
            zoom: 8,
        });
        var marker = new google.maps.Marker({
        position: myCenter,
        map: map,
        title: '2550 Tamalpais Avenue El Cerrito CA 94530'
        });
      }
    </script>
    <script async defer
        src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDkGLT05AiqH5PAqdTUt83prNqXbEGu2Z4&callback=initMap">
